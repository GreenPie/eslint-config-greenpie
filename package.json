{
  "name": "eslint-config-greenpie",
  "version": "15.1.1",
  "description": "GreenPie's ESLint config file",
  "main": "index.js",
  "author": "Roman Nuritdinov (Ky6uk)",
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@11.10.1",
  "homepage": "https://github.com/GreenPie/eslint-config-greenpie",
  "repository": {
    "type": "git",
    "url": "https://github.com/GreenPie/eslint-config-greenpie"
  },
  "bugs": {
    "url": "https://github.com/GreenPie/eslint-config-greenpie/issues"
  },
  "scripts": {
    "lint:eslint": "eslint",
    "lint:oxlint": "oxlint",
    "lint": "npm run lint:oxlint && npm run lint:eslint",
    "prepare": "husky",
    "test:agent": "vitest run --reporter=json",
    "test": "vitest run",
    "update:all": "taze -I -l --peer"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "greenpie",
    "config",
    "javascript",
    "styleguide",
    "typescript",
    "vue",
    "oxlint"
  ],
  "devDependencies": {
    "@types/node": "^25.3.0",
    "execa": "^9.6.1",
    "husky": "^9.1.7",
    "oxlint": "^1.50.0",
    "taze": "^19.9.2",
    "valibot": "1.2.0",
    "vitest": "^4.0.18",
    "vue": "^3.5.28"
  },
  "peerDependencies": {
    "@stylistic/eslint-plugin": "^5.9.0",
    "eslint": "^10.0.1",
    "eslint-plugin-vue": "^10.8.0",
    "oxlint": "^1.50.0",
    "typescript-eslint": "^8.56.0"
  }
}
