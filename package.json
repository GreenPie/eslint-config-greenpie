{
  "name": "eslint-config-greenpie",
  "version": "15.0.1-3",
  "description": "GreenPie's ESLint config file",
  "main": "index.js",
  "author": "Roman Nuritdinov (Ky6uk)",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/GreenPie/eslint-config-greenpie",
  "repository": {
    "type": "git",
    "url": "https://github.com/GreenPie/eslint-config-greenpie"
  },
  "bugs": {
    "url": "https://github.com/GreenPie/eslint-config-greenpie/issues"
  },
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:oxlint": "oxlint --type-aware",
    "lint": "eslint .",
    "prepare": "npm run lint:eslint && npm run lint:oxlint",
    "update:all": "taze -I -l --peer"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "greenpie",
    "config",
    "javascript",
    "styleguide",
    "typescript",
    "vue",
    "oxlint"
  ],
  "devDependencies": {
    "oxlint": "^1.32.0",
    "taze": "^19.9.2"
  },
  "peerDependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "eslint-plugin-vue": "^10.6.2",
    "eslint": "^9.39.2",
    "oxlint-tsgolint": "^0.9.0",
    "oxlint": "^1.32.0",
    "taze": "^19.9.2",
    "typescript-eslint": "^8.49.0"
  }
}
