{
  "name": "eslint-config-greenpie",
  "version": "15.0.1-19",
  "description": "GreenPie's ESLint and oxlint configs",
  "main": "index.js",
  "author": "Roman Nuritdinov (Ky6uk)",
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@11.10.0",
  "homepage": "https://github.com/GreenPie/eslint-config-greenpie",
  "repository": {
    "type": "git",
    "url": "https://github.com/GreenPie/eslint-config-greenpie"
  },
  "bugs": {
    "url": "https://github.com/GreenPie/eslint-config-greenpie/issues"
  },
  "scripts": {
    "lint:eslint": "eslint",
    "lint:oxlint": "oxlint --type-aware",
    "lint": "npm run lint:oxlint && npm run lint:eslint",
    "prepare": "husky",
    "update:all": "taze -I -l --peer"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "greenpie",
    "config",
    "javascript",
    "styleguide",
    "typescript",
    "vue",
    "oxlint"
  ],
  "devDependencies": {
    "husky": "^9.1.7",
    "oxlint": "^1.47.0",
    "taze": "^19.9.2",
    "vitest": "^4.0.18",
    "vue": "^3.5.28"
  },
  "peerDependencies": {
    "@stylistic/eslint-plugin": "^5.8.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "oxlint": "^1.47.0",
    "oxlint-tsgolint": "^0.12.1",
    "typescript-eslint": "^8.55.0"
  }
}
